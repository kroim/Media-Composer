<?xml version="1.0" encoding="UTF-8"?>

<view type="" id="">
  <bounds x="0" y="0" width="500" height="300"/>
  <image id="background" src="downmixer_bg_stereo-mono.png" x="0" y="0"/>

  <!--  L  -->

  <view type="Slider" id="L Gain">
    <parameter id="L Gain"/>
    <bounds x="124" y="60" width="25" height="220"/>
    <image_list id="cap" src="slidercap_vertical_blue.png" x="0" y="0"/>
    <image_list id="cap_highlight" x="3" y="2">
      <image id="" src="slidercap_vertical_blue.png"/>
      <image id="blue" src="auto_slider_vertical_blue.png"/>
      <image id="red" src="auto_slider_vertical_red.png"/>
      <image id="green" src="auto_slider_vertical_green.png"/>
      <image id="yellow" src="auto_slider_vertical_yellow.png"/>
    </image_list>
    <range min="0.0" max="1.0" interval="0.001"/>
  </view>

  <view type="EditText" id="L Gain Text">
    <parameter id="L Gain"/>
    <bounds x="125" y="44" width="52" height="19"/>
    <tooltip string=""/>
    <image id="hit_area" src="" x="0" y="0"/>
    <font typeface="Arial" height="10" bold="1" italic="0" underlined="0"/>
    <color id="text" red="233" green="241" blue="249" alpha="255"/>
    <justification horizontal="center" vertical="center"/>
    <inset top="2" left="2" bottom="2" right="2"/>
    <text string="0"/>
  </view>

  <view type="Button" id="L Mute">
    <parameter id="L Mute"/>
    <bounds x="119" y="20" width="34" height="21"/>
    <image_list id="state">
      <image id="" src="downmixer_btn_mute_off.png"/>
	  <image id="" src="downmixer_btn_mute_on.png"/>
    </image_list>
    <image_list id="highlight">
      <image id="blue" src="auto_btn_mute_blue.png"/>
      <image id="red" src="auto_btn_mute_red.png"/>
      <image id="green" src="auto_btn_mute_green.png"/>
      <image id="yellow" src="auto_btn_mute_yellow.png"/>
    </image_list>
    <toggles value="true"/>
  </view>

  <view type="Button" id="L Phase">
    <parameter id="L Phase"/>
    <bounds x="149" y="20" width="34" height="21"/>
    <image_list id="state">
